<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="350dp"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#FAFAFA"
    android:padding="5dp"
    android:layout_gravity="center"
    tools:context=".activity.dosbim.PopupDosbimAbsensiMahasiswa">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:layout_width="50px"
            android:layout_height="60px"
            android:background="@drawable/edit_icon"
            android:id="@+id/btn_edit_data_aslab_kalab"
            android:layout_marginLeft="400px"
            android:layout_weight="1"/>

        <Button
            android:layout_width="50px"
            android:layout_height="60px"
            android:background="@drawable/edit_icon"
            android:layout_gravity="end"
            android:id="@+id/btn_delete_data_nilai_mahasiswa_dosbim"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="40px">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="Nama          "
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_nama_mhs_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="2"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="NBI              "
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_nip_mhs_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="2"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="Pertemuan 1"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_pertemuan1_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="4"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="Pertemuan 2"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_pertemuan2_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="2"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="Pertemuan 3"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_pertemuan3_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="2"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50px"
            android:text="Pertemuan 4"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="50px"
            android:text=""
            android:id="@+id/txt_detail_pertemuan4_dosbim"
            android:textSize="14sp"
            android:fontFamily="@font/nunitosans_bold"
            android:textColor="#000000"
            android:layout_weight="2"/>
    </LinearLayout>
</LinearLayout>