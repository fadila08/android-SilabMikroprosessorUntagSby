        <LinearLayout
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btdatalaborankalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imagestrukturorg"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Data\nLaboran"
                    android:textSize="10sp" />

                <Button
                    android:id="@+id/btdataaslabkalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="25dp"
                    android:layout_toRightOf="@+id/btdatalaborankalab"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imagestrukturorg"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Data\nAslab"
                    android:textSize="10sp" />

                <Button
                    android:id="@+id/btdatadosbimkalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="25dp"
                    android:layout_toRightOf="@+id/btdataaslabkalab"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imagestrukturorg"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Data\nDosbim"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/btdatamhskalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imagestrukturorg"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Data\nMahasiswa"
                    android:textSize="10sp" />

                <Button
                    android:id="@+id/btnilaimhskalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="25dp"
                    android:layout_toRightOf="@+id/btdatamhskalab"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imagenilai"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Nilai\nMahasiswa"
                    android:textSize="10sp" />

                <Button
                    android:id="@+id/btabsmhskalab"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="25dp"
                    android:layout_marginLeft="23dp"
                    android:layout_toRightOf="@+id/btnilaimhskalab"
                    android:background="@drawable/bgputihwaemail"
                    android:drawableTop="@drawable/imageabsensi"
                    android:fontFamily="@font/nunito_extrabold"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:text="Absensi\nMahasiswa"
                    android:textSize="10sp" />
            </RelativeLayout>

            <RelativeLayout
                android:gravity="center"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
