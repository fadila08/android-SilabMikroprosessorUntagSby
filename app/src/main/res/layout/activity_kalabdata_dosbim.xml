                android:layout_marginStart="35dp"
                android:layout_marginEnd="35dp"
                android:layout_below="@+id/tvsemester"
                android:id="@+id/spinsemester"
                android:layout_width="match_parent"
                android:layout_height="30dp">
            </Spinner>

            <Button
